

## SNP calling with refmap.pl in Stacks
#!/bin/sh
#$ -S /bin/bash
#$ -cwd
#$ -l s_vmem=100G
#$ -l mem_req=100G
#$ -l d_rt=960:00:00
#$ -l s_rt=960:00:00

singularity exec /usr/local/biotools/s/stacks\:2.60--h9a82719_0 ref_map.pl -T 3 -o /home/mmorita0606/masaya/find_predicted_proteins/Acropora_digitifera_Larvae23/refmap_larvae  --popmap list_Acropora_larvae_2.txt --samples /home/mmorita0606/masaya/find_predicted_proteins/Acropora_digitifera_Larvae23/bam/ -X "populations: --plink" -X "populations: --phylip-var" -X "populations: --vcf" -X "populations: --treemix"
